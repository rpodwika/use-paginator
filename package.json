{
  "name": "use-paginator",
  "version": "0.1.0",
  "description": "Hook for a simple pagination",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpodwika/use-paginator.git"
  },
  "keywords": [
    "react",
    "pagination",
    "paginator",
    "hook",
    "react hooks",
    "typescript",
    "npm",
    "paginator-hook",
    "pagination-hook",
    "pages"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "author": "Robert Podwika",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpodwika/use-paginator/issues"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "homepage": "https://github.com/rpodwika/use-paginator#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "rollup": "^2.39.1",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "files": [
    "dist"
  ]
}
